<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>MIPL.Utility.AppLogger</PackageId>
    <Title>AppLogger</Title>
    <Authors>MIPL</Authors>
    <Company>MIPL</Company>
    <Product>AppLogger</Product>
    <Version>1.0.0</Version>
    <Description>Library to Log text to console</Description>
    <RepositoryUrl>https://github.com/digvijayghatge07/AppLogger</RepositoryUrl>
    <PackageTags>Logging, Logger, Logs</PackageTags>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>BUILD;DEBUG;RELEASE;</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>BUILD;DEBUG;RELEASE;</DefineConstants>
  </PropertyGroup>

</Project>
